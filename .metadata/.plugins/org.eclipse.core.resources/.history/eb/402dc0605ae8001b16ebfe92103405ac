import java.util.*;
public class Prog4 {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int num;
		num = in.nextInt();
		for(int i=0;i<=num;i++) {
			System.out.print("*");
			if (i > 0) {
				for(int j=0;j<=i;j++) {
					System.out.print(j);
				}
				for(int k=num;k>0;k--) {
					System.out.print(k);
				}
				System.out.println("*");
		   }
		}
	}
}
