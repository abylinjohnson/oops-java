import java.util.*;
public class Prog1 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int num;
		System.out.println("Enter the Length Of the Array:");
		num = sc.nextInt();
		int arr[];
		arr = new int[num]; 
		System.out.println("Enter The numbers:");
		for(int i=0;i<num;i++)
			arr[i] = sc.nextInt();
		
		
	}

}
